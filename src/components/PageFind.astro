---
import '@pagefind/default-ui/css/ui.css';

---


<div id="search" transition:persist=""></div>
<script>
    import { PagefindUI } from '@pagefind/default-ui';
    window.addEventListener("DOMContentLoaded", () => {
        new PagefindUI({ 
            element: "#search",
            showImages: true,
            showEmptyFilters: false
        });
    });
</script>